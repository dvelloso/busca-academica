    "import pandas as pd\n",
    "import bibtexparser\n",
    "\n",
    "\n",
    "with open('dados/ACM1.bib', encoding='utf8') as bibtex_file:\n",
    "    bib_database = bibtexparser.load(bibtex_file)\n",
    "    df = pd.DataFrame(bib_database.entries)\n",
    "\n",
    "df\n",
    "\n",
    "df.rename(columns={'ENTRYTYPE': 'type_publication'}, inplace=True)\n",
    "\n",
    "df"
   ]
